@model List<Prueba1.Models.ViajesViewModel>
@{
    ViewBag.Title = "Detalle";
}
<h2>Reporte Detallado</h2>

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>Colaborador</th>
                <th>Fecha de Viaje</th>
                <th>Recorrido (KM)</th>
                <th>Pago por Viaje</th>
            </tr>
            <tbody id="tbody">
                @foreach (var ViajeDetalle in Model)
                {
                    <tr>
                        <td>@ViajeDetalle.Colaborador</td>
                        <td>@ViajeDetalle.Fecha</td>
                        <td>@ViajeDetalle.Recorrido</td>
                        <td>@ViajeDetalle.Pago</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

